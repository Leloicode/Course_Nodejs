<div class="products mt-4">
  <div class="row">
    {{#each data}}
      <div class="col-sm-6 col-lg-4">

        <div class="card" style="width: 18rem;">
          <a href="/products/{{this.slug}}"><img
              class="card-img-top"
              src="{{this.image}}"
              alt="Card image cap"
            /></a>
          <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <p class="card-text">{{this.price}}</p>
            <a href="#" class="btn btn-primary">GO TO BUY</a>
            <span><button class="btn btn-dark">Delete</button></span>
            <span><a
                href="/products/{{this._id}}/edit"
                class="btn btn-dark"
              >Edit</a></span>
          </div>
        </div>
      </div>
    {{/each}}
  </div>
</div>